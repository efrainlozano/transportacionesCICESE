<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.transporte.cicese.transportaciones_cicese.AtajosActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView15"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="487dp">

        <Button
            android:id="@+id/irARegistros"
            android:layout_width="120dp"
            android:layout_height="90dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="16dp"
            android:text="Registrar usuarios y solicitudes">

        </Button>


        <Button
            android:id="@+id/irAUpdate"
            android:layout_width="120dp"
            android:layout_height="90dp"
            android:layout_alignTop="@+id/irARegistros"
            android:layout_centerHorizontal="true"
            android:text="Actualizar servicios">

        </Button>


        <Button
            android:id="@+id/irAConsultas"
            android:layout_width="120dp"
            android:layout_height="90dp"
            android:layout_alignTop="@+id/irAUpdate"
            android:layout_marginStart="11dp"
            android:layout_toEndOf="@+id/irAUpdate"
            android:text="Consultar servicios">

        </Button>
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
